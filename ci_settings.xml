<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
  <servers>
    <server>
      <id>ci-gitlab-maven</id>
      <configuration>
        <httpHeaders>
          <property>
            <name>Job-Token</name>
            <value>${env.CI_JOB_TOKEN}</value>
          </property>
        </httpHeaders>
      </configuration>
    </server>
  </servers>
  <profiles>
  <profile>
   <id>ci-gitlab-maven</id>
      <properties>
    <altSnapshotDeploymentRepository>ci-gitlab-maven::${CI_API_V4_URL}/projects/${env.CI_PROJECT_ID}/packages/maven</altSnapshotDeploymentRepository>
    <altReleaseDeploymentRepository>ci-gitlab-maven::${CI_API_V4_URL}/projects/${env.CI_PROJECT_ID}/packages/maven</altReleaseDeploymentRepository>
   </properties>
   <repositories>
    <repository>
     <id>ci-gitlab-maven</id>
     <url>${env.CI_API_V4_URL}/projects/${env.CI_PROJECT_ID}/packages/maven</url>
    </repository>
   </repositories>
  </profile>
  </profiles>
</settings>